{
	"name": "@dependency/deploymentProvisioning",
	"version": "1.0.7",
	"main": "./entrypoint/programmaticAPI",
	"scripts": {
		"exportGraphData": "yarn run scriptManager shouldCompileScript=true @dependency/deploymentScript \".graphDataLoading.exportAllGraphData({targetPath: './temporary/resource',fileName: 'graphData.json'})\"",
		"loadGraphData": "yarn run scriptManager shouldCompileScript=true @dependency/deploymentScript \".graphDataLoading.loadGraphDataFromFile({ shouldClearDatabase: true, graphDataFilePath: './resource/graphData.json' })\"",
		"build": "yarn run scriptManager shouldCompileScript=true @dependency/deploymentScript \".buildSourceCode.moduleProject({}, {compile:[/**'./test'*/]})\"",
		"buildAndRelease": "yarn run scriptManager shouldCompileScript=true @dependency/deploymentScript \".buildAndRelease.moduleProject({})\"",
		"runNeo4jContainer": "yarn run scriptManager shouldCompileScript=true ./source/dockerContainer/neo4jContainer.js \".runDockerContainer()\""
	},
	"dependencies": {
		"any-promise": "^1.3.0",
		"command-exists": "^1.2.8",
		"gulp-plumber": "^1.2.1",
		"gulp-size": "^3.0.0",
		"is-wsl": "*",
		"mkdirp": "^0.5.1",
		"rsync": "^0.6.1",
		"shelljs": "^0.8.3",
		"vinyl-fs": "^3.0.3",
		"which": "*"
	},
	"devDependencies": {
		"@dependency/deploymentScript": "https://github.com/AppScriptIO/deploymentScript#semver:^1.x.x",
		"@dependency/javascriptStaticAnalysis": "https://github.com/AppScriptIO/javascriptStaticAnalysis#semver:^1.x.x",
		"@dependency/scriptManager": "https://github.com/AppScriptIO/scriptManager#semver:^1.x.x",
		"typescript": "^3.3.3333"
	}
}